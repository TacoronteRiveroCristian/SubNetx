{
    "name": "nextjs-devcontainer",
    "version": "1.0.0",
    "scripts": {
        "dev": "next dev -p 3200",
        "build": "next build",
        "start": "next start -p 3200",
        "lint": "next lint",
        "postinstall": "prisma generate",
        "db:push": "prisma db push",
        "db:init": "ts-node scripts/init-db.ts",
        "db:setup": "npm run db:push && npm run db:init",
        "dev:setup": "npm run db:setup && npm run dev"
    },
    "dependencies": {
        "@prisma/client": "^6.5.0",
        "bcryptjs": "^3.0.2",
        "cookie": "^0.6.0",
        "jsonwebtoken": "^9.0.2",
        "next": "14.0.0",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.6",
        "@types/cookie": "^0.6.0",
        "@types/jsonwebtoken": "^9.0.5",
        "@types/node": "^18.19.84",
        "@types/react": "^18.0.0",
        "@types/react-dom": "^18.0.0",
        "autoprefixer": "^10.0.1",
        "eslint": "^8",
        "eslint-config-next": "14.1.0",
        "postcss": "^8",
        "prisma": "^6.5.0",
        "tailwindcss": "^3.3.0",
        "ts-node": "^10.9.2",
        "typescript": "^5.8.2"
    }
}
